{
  "repositoryUrl": "https://github.com/kiriyaga/test-worklows.git",
  "branches": "main",
  "debug": true,
  "addReleases": "top",
  "releaseRules": [
    {"breaking": true, "release": "minor"}
  ],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    [
      "@semantic-release/changelog",
      {
        "changelogFile": "packages/hardhat-zksync-chai-matchers/CHANGELOG.md"
      }
    ],
    [
      "@semantic-release/git",
      {
        "assets": ["packages/hardhat-zksync-chai-matchers/CHANGELOG.md", "packages/hardhat-zksync-chai-matchers/package.json"],
        "message": "${nextRelease.version} version bump [skip ci]\n\n${nextRelease.notes}"
      }
    ],
    "@semantic-release/github"
  ]
}